@Article{Teo2002c,
  author    = {Swee-Ann Teo and Mook-Seng Leong and Siou-Teck Chew and Ban-Leong Ooi},
  title     = {{Complete location of poles for thick lossy grounded dielectric slab}},
  journal   = {{IEEE} Transactions on Microwave Theory and Techniques},
  year      = {2002},
  volume    = {50},
  number    = {2},
  pages     = {440--445},
  issn      = {00189480},
  abstract  = {In order to obtain accurate closed-form representations of the microstrip Green's functions, it is often necessary to find the locations of the proper and improper surface-wave poles. In this paper, we present an efficient and robust iterative algorithm based on contraction mapping, which can locate all the proper and improper solutions of the characteristic equations of the grounded dielectric slab. The dielectric may also be lossy},
