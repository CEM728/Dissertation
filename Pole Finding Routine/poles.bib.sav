@Article{Teo2003b,
  author    = {Teo, Swee Ann and Chew, Siou Teck and Leong, Mook Seng},
  title     = {{Error analysis of the discrete complex image method and pole extraction}},
  journal   = {{IEEE} Transactions on Microwave Theory and Techniques},
  year      = {2003},
  volume    = {51},
  number    = {2 I},
  pages     = {406--413},
  month     = feb,
  issn      = {00189480},
  abstract  = {Recently, Teo et al (IEEE AP-S Int. Symp., vol. 4 pp. 862-864 2001) proposed a discrete complex image method that is able to represent both the near and far fields of the Green's function accurately, using only a single approximate Green's function. In this paper, an error analysis of this method is presented. The error analysis shows an additional term which represents the quasi-static image of the surface-wave poles plays a crucial role in the convergence and accuracy of this new method. As the extraction of poles is required, a recently proposed algorithm by Teo et al (IEEE Trans. Microwave Theory Tech., vol. 50 pp. 440-445 2002) is also discussed and compared with existing methods in terms of computational speed.},
  doi       = {10.1109/TMTT.2002.807834},
  file      = {:TEO - Error Analysis of DCIM and pole extraction.pdf:pdf},
  keywords  = {Discrete complex image,Discrete complex image method (DCIM),Green's function,Proper poles,Sommerfeld transform,Surface-wave poles},
  publisher = {Institute of Electrical and Electronics Engineers ({IEEE})},
}

@Article{Chen2016b,
  author        = {Parry Y. Chen and Yonatan Sivan},
  title         = {{Robust Location of Optical Fiber Modes via the Argument Principle Method}},
  journal       = {Computer Physics Communications},
  year          = {2017},
  volume        = {214},
  number        = {2},
  pages         = {24},
  month         = {may},
  abstract      = {We implement a robust, globally convergent root search method for transcendental equations guaranteed to locate all complex roots within a specified search domain, based on Cauchy's residue theorem. Although several implementations of the argument principle already exist ours has several advantages: it allows singularities within the search domain and branch points are not fatal to the method. Furthermore, our implementation is simple and is written in MATLAB, fulfilling the need for an easily integrated implementation which can be readily modified to accommodate the many variations of the argument principle method, each of which is suited to a different application. We apply the method to the step index fiber dispersion relation which has become topical due to the recent proliferation of high index contrast fibers. We also find modes with permittivity as the eigenvalue catering to recent numerical methods that expand the radiation of sources by eigenmodes.},
  archiveprefix = {arXiv},
  arxivid       = {1609.01956},
  date          = {2016-09-07},
  doi           = {10.1016/j.cpc.2016.11.009},
  eprint        = {1609.01956v1},
  eprintclass   = {physics.comp-ph},
  eprinttype    = {arXiv},
  file          = {online:http\:\\arxiv.org\\pdf\\1609.01956v1:PDF},
  keywords      = {physics.comp-ph, physics.optics},
  publisher     = {Elsevier {BV}},
  url           = {http://arxiv.org/abs/1609.01956},
}

