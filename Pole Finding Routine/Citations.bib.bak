@Article{Kravanja2000c,
  author    = {P. Kravanja and M. Van Barel and O. Ragos and M.N. Vrahatis and F.A. Zafiropoulos},
  title     = {{ZEAL}: A Mathematical Software Package for Computing Zeros of Analytic Functions},
