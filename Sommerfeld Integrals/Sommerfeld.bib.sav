  file          = {:D\:\\Users\\Mendeley Desktop\\Amit Lal - OpenSIM.pdf:pdf},
  keywords      = {Image reconstruction,Lighting,Microscopy,Optical imaging,Optical microscopy,SIM,Spatial resolution,Structured illumination,optical transfer function,super-resolution},
}

@Article{Steyaert2014,
