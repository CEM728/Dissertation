@Article{Popov2005,
  author   = {Popov, V V and Tsymbalov, G M and Shur, M S and Knap, W},
  title    = {{The resonant terahertz response of a slot diode with a two-dimensional electron channel}},
  journal  = {Semiconductors},
  year     = {2005},
  volume   = {39},
  number   = {1},
  pages    = {142--146},
  issn     = {10637826},
  abstract = {The terahertz response of a slot diode with a two-dimensional electron channel is calculated on the basis of the first principles of electromagnetism. It is shown that all characteristic electromagnetic lengths (scattering, absorption, and extinction lengths), as well as the impedance of the diode, exhibit resonances at plasmon excitation frequencies in the channel. The fundamental resonance behaves similarly to the current resonance in an RLC circuit. It has been concluded that, even at room temperature, a slot diode with a two-dimensional electron channel provides a resonant circuit at terahertz frequencies that couples effectively to external electromagnetic radiation with a loaded Q-factor exceeding unity. The diode resistance may be measured from contactless measurements of the characteristic electromagnetic lengths of the diode. (C) 2005 Pleiades Publishing, Inc.},
  doi      = {10.1134/1.1852665},
  file     = {:D\:\\Users\\Mendeley Desktop\\Popove - with diserpsion.pdf:pdf},
  isbn     = {1063-7826},
  keywords = {absorption,field-effect transistors,fluid,gas,mobility transistors,modes,photoconductivity,plasma-waves,radiation,subterahertz},
