@Article{Sydoruk2015,
  author   = {Sydoruk, Oleksiy and Choonee, Kaushal and Dyer, Gregory C},
  title    = {{Transmission and Reflection of Terahertz Plasmons in Two-Dimensional Plasmonic Devices}},
  journal  = {IEEE transactions on THz science and technology},
  year     = {2015},
  volume   = {5},
  number   = {3},
  pages    = {486--496},
  abstract = {—Plasmons in two-dimensional semiconductor devices will be reflected by discontinuities, notably, junctions between gated and non-gated electron channels. The transmitted and reflected plasmons can form spatially-and frequency-varying signals, and their understanding is important for the design of terahertz detectors, oscillators, and plasmonic crystals. Using mode decomposition, we studied terahertz plasmons incident on a junction between a gated and a nongated channel. The plasmon reflection and transmission coefficients were found numerically and analytically and studied between 0.3 and 1 THz for a range of electron densities. At higher frequencies, we could describe the plasmons by a simplified model of channels in homogeneous dielectrics, for which the analytical approximations were accurate. At low frequencies, however, the full geometry and mode spectrum had to be taken into account. The results agreed with simulations by the finite-element method. Mode decomposition thus proved to be a powerful method for plasmonic devices, combining the rigor of complete solutions of Maxwell's equations with the convenience of analytical expressions.},
  doi      = {10.1109/TTHZ.2015.2405919},
  file     = {:D\:\\Users\\Mendeley Desktop\\TL Model of 2DEG.pdf:pdf;:D\:\\Users\\Mendeley Desktop\\Sydoruk, Choonee, Dyer - 2015 - Transmission and Reflection of Terahertz Plasmons in Two-Dimensional Plasmonic Devices.pdf:pdf;:D\:\\Users\\Mendeley Desktop\\Sydoruk, Choonee, Dyer - 2015 - Transmission and Reflection of Terahertz Plasmons in Two-Dimensional Plasmonic Devices(2).pdf:pdf},
  keywords = {,2DEG,Index Terms—Mode decomposition,plasmon,two-dimensional channel},
}

